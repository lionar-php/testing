{
	"name": "lionar/testing",
	"version": "0.0.3",
	"description": "This package consists of files that are regularly used for unit testing.",
	"keywords": [ "Lionar", "Unit testing" ],
	"license": "MIT",
	"require" : {
		"filp/whoops"			: "*",
		"hamcrest/hamcrest-php"	: "*",
		"mikey179/vfsStream" 	: "dev-master",
		"mockery/mockery"		: "@stable",
		"phpunit/phpunit"		: "@stable",
		"symfony/css-selector"	: "@stable", 
		"symfony/dom-crawler"	: "@stable"
  	},
	"autoload": {
    	"psr-4": {
			"Testing\\": "source"
		},
		"files": [
			"../../hamcrest/hamcrest-php/hamcrest/Hamcrest.php",
			"helpers.php",
			"whoops.php"
		]
  	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}